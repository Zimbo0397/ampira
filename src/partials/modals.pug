.main-modal.comment-modal
	.modal-inner
		.modal-top
			.close-btn
				i.fa.fa-times(aria-hidden="true")
		.form-inner
			form(action="#")
				input(type="text", placeholder="name")
				input(type="text", placeholder="company")
				.file
					span.button add photo
					input(type="file")
				textarea#345f(placeholder="comment")

